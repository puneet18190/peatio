<tr data-order='<%= @index %>' data-price='<%= @price %>' data-volume='<%= @volume %>' class='<%= @classes %>'>
  <td class='price col-md-4 text-down'><div>
    <%- formatter.mask_fixed_price @price %>
  </div></td>
  <td class='volume col-md-4'><div>
    <%- formatter.mask_fixed_volume @volume %>
  </div></td>
  <td class='amount col-md-4'><div>
    <%- formatter.amount @volume, @price %>
  </div></td>
</tr>
