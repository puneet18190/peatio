  body.animsition
    #searchbar.collapse.searchbar
      .search-area
        .container
          .row
            .col-lg-12.col-md-12.col-sm-12.col-xs-12
              .input-group
                input.form-control placeholder=("Search for...") type="text" /
                span.input-group-btn
                  button.btn.btn-default type="button"  Go!
              /! /input-group
            /! /.col-lg-6
    .top-bar
      /! top-bar
      .container
        .row
          .col-md-7.hidden-xs.hidden-sm
            .top-nav
              span.top-text-left.hidden-xs.homeExchange
